<Window x:Class="LoveLetter_GruppeOpgave.Views.HomeScreenViews"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LoveLetter_GruppeOpgave.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Window.DataContext>
        <local:HomeViewModel/>
    </Window.DataContext>
    
    <Grid Background="White">
        <ListView ItemsSource="{Binding lobbies, UpdateSourceTrigger=Default, Mode=OneWay}" HorizontalAlignment="Center" VerticalAlignment="Center">
        </ListView>
    </Grid>
</Window>
